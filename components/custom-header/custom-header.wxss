
.navigation-container {
    position: fixed;
    width: 100%;
    z-index: 99;
    top: 0;
    left: 0;
    background-color: #ffffff;
    display: flex;
    flex-direction: column;
}

.custom-header {
    height: 140rpx;
    display: flex;
    align-items: center;
}

.header-left {
    position: absolute;
    left: 32rpx;
    bottom: 44rpx;
    transform: translateY(50%);
    height: 64rpx;
    width: 100rpx;
    display: flex;
    align-items: center;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

.left-c {
    width: 14.67rpx;
    height: 25.33rpx;
}

.home-c {
    width: 61.33rpx;
    height: 61.33rpx;
}

.home-a {
    width: 40rpx;
    height: 40rpx;
}

.left-image {
    width: 40rpx;
    height: 40rpx;
}

.header-center {
    position: absolute;
    left: 208rpx;
    right: 208rpx;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.header-centers {
    position: absolute;
    bottom: 44rpx;
    left: 100rpx;
    transform: translateY(50%);
    font-size: 14px;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.page {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    overflow-y: scroll;
}
